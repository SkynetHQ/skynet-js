{
  "name": "skynet-js",
  "version": "4.2.1-beta",
  "description": "Sia Skynet Javascript Client",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "browserslist": [
    "defaults",
    "not IE 11",
    "not OperaMini all"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json && tsc --project tsconfig.build.cjs.json",
    "lint": "yarn lint:tsc && yarn lint:eslint",
    "lint:eslint": "eslint --ext .ts . --max-warnings 0",
    "lint:tsc": "tsc",
    "prepublishOnly": "yarn && yarn build",
    "test": "jest --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkynetLabs/skynet-js.git"
  },
  "keywords": [
    "sia",
    "skynet"
  ],
  "author": "Karol Wypchlo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SkynetLabs/skynet-js/issues"
  },
  "homepage": "https://github.com/SkynetLabs/skynet-js",
  "dependencies": {
    "@skynetlabs/tus-js-client": "^2.4.0",
    "async-mutex": "^0.3.2",
    "axios": "^0.27.2",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.1.1",
    "base64-js": "^1.3.1",
    "blakejs": "^1.1.0",
    "buffer-from": "^1.1.2",
    "mime": "^3.0.0",
    "path-browserify": "^1.0.1",
    "pbkdf2-hmac": "^1.0.4",
    "post-me": "^0.4.5",
    "skynet-mysky-utils": "^0.3.0",
    "tweetnacl": "^1.0.3",
    "url-join": "^4.0.1",
    "url-parse": "^1.5.1"
  },
  "devDependencies": {
    "@types/base64-js": "^1.3.0",
    "@types/buffer-from": "^1.1.0",
    "@types/jest": "^27.0.1",
    "@types/mime": "^2.0.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^15.0.1",
    "@types/pbkdf2": "^3.1.0",
    "@types/url-join": "^4.0.0",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "axios-mock-adapter": "^1.18.2",
    "axios-retry": "^3.2.5",
    "eslint": "^7.11.0",
    "eslint-plugin-jsdoc": "^39.2.8",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^12.0.2",
    "minimist": "^1.2.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
